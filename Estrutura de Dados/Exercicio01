import collections
d = collections.deque()
print (d)
d.append('a')
d.append('b')
print(d)
d.appendleft ('b')
print(d)
d.extend('cdef')
print(d)
d.extendleft('ghij')
print(d)
d.insert(5,'k')
print(d)
d.pop()
print(d)
d.popleft()
print(d)
d.remove('b')
print(d)
d.reverse()
print(d)
d.rotate()
print(d)
d.clear()
print(d)
d3 = collections.deque(maxlen=30)
print(d3)